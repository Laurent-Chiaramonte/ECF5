@model IEnumerable<MaintInfoBo.TypeModele>

@{
    ViewBag.Title = "Index";
}

<h2>Liste des Types de Modèle :</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayName("Désignation")
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.designation_type)
        </td>
    </tr>
}

</table>

<div>
    @Html.ActionLink("Retour à l'accueil", "Index", "Home")
</div>
